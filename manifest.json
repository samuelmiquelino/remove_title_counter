{
    "manifest_version" : 2,
    "name" : "Remove Title Counter",
    "description" : "Removes notification counter on YouTube tabs",
    "version" : "0.1",
    "icons": { "16": "icon16.png",
        "48": "icon48.png",
       "128": "icon128.png" },
    "content_scripts" : [
        {
            "matches" : ["*://www.youtube.com/*"],
            "js" : ["content.js"]
        }
    ]
}